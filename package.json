{
  "name": "redux-heartbeat",
  "version": "0.3.1",
  "description": "Redux middleware providing a heartbeat that contains log of actions occurring between each beat. Great for hooking up further tools for reporting application usage over time, or persisting replayable sessions incrementally.",
  "repository": {
    "type": "git",
    "url": "https://github.com/alechill/redux-heartbeat.git"
  },
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "preversion": "npm run test",
    "prepublish": "npm run test",
    "compile": "tsc --project . --module commonjs",
    "pretest": "npm run compile",
    "test": "mocha --require babel-core/register ./dist/test/test.js"
  },
  "keywords": [
    "redux",
    "middleware",
    "heartbeat"
  ],
  "author": "Alec Hill",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "@types/sinon": "^7.5.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "pre-commit": "^1.2.2",
    "redux": "^4.0.4",
    "sinon": "^7.5.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
